
package controls.controls;

import controls.icontrols.*;

public class Controls implements IConnect{

	IConnect	connect;
	IHandle		handle;

	public Controls() {

		super();
		// TODO Auto-generated constructor stub
	}

	public Controls(IConnect connect, IHandle handle) {

		super();
		this.connect = connect;
		this.handle = handle;
	}

	@Override
	public boolean openConnect() throws Exception {

		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public boolean closeConnect() throws Exception {

		// TODO Auto-generated method stub
		return false;
	}

}
